{
  "framework": "nextjs",
  "buildCommand": "NEXT_TELEMETRY_DISABLED=1 next build",
  "installCommand": "npm install",
  "regions": ["sfo1"],
  "github": {
    "silent": true
  },
  "redirects": [
    {
      "source": "/github",
      "destination": "https://github.com/alhridoy/mcp-wtf"
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  }
}
